<% provide(:title, "All Invite Codes") %>

	<%= link_to root_path do %>
<div class="all_logo">
	<%= image_tag("headerlogo.png") %>	
	
</div>
	<% end %>

	<ul class="admin_nav_bar">
		<li> <%= link_to "Profile", current_admin %></li>
		<li> <%= link_to "Settings", edit_admin_path(current_admin) %></li>
		<li> <%= link_to "Log Out", logout_path, method: "delete" %> </li>
	</ul>
	
	<% flash.each do |key, value| %>
	  <div class="alert  alert-<%= key %>"><%= value %></div>
	<% end %>

<%= render 'shared/allbar' %>
<%= render 'shared/sidebar' %>
	
	<div class="admin_show">
		
		
		<h1 class="invite_all_header">All Invite Codes</h1>
		<% @invites.each do |invite| %>
		<p class="all_codes"> <%= invite.code %> </p>
		<% end %>

	
	</div>
