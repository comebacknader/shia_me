<% provide(:title, @admin.name) %>

	<%= link_to root_path do %>
<div class="all_logo">
	<%= image_tag("headerlogo.png") %>	
	
</div>
	<% end %>

	<ul class="admin_nav_bar">
		<li> <%= link_to "Profile", current_admin %></li>
		<li> <%= link_to "Settings", edit_admin_path(@admin) %></li>
		<li> <%= link_to "Log Out", logout_path, method: "delete" %> </li>
	</ul>


<div class="all_bar">

	<div class="matches_button">
		<p> Your Matches </p>
	</div>

	<%= link_to admins_path do %>
		<div class="home_button">
			<p> Home </p>
		</div>
	<% end %>

</div>

<div class="admin_sidebar">
</div>


	<% flash.each do |key, value| %>
	  <div class="alert  alert-<%= key %>"><%= value %></div>
	<% end %>	
	
	<div class="admin_show">
		
		
		<h1 class="admin_show_header"><%= @admin.name %>'s Dashboard</h1>

	
	</div>
