<% provide(:title, @admin.name) %>

	<%= link_to root_path do %>
<div class="all_logo">
	<%= image_tag("headerlogo.png") %>	
	
</div>
	<% end %>

	<ul class="admin_nav_bar">
		<li> <%= link_to "Profile", current_admin %></li>
		<li> <%= link_to "Settings", edit_admin_path(@admin) %></li>
		<li> <%= link_to "Change Picture", pics_admin_path %></li>
		<li> <%= link_to "Log Out", logout_path, method: "delete" %> </li>
	</ul>


<%= render 'shared/allbar' %>

<%= render 'shared/sidebar' %>


	<% flash.each do |key, value| %>
	  <div class="alert  alert-<%= key %>"><%= value %></div>
	<% end %>	
	
	<div class="admin_show">
		
		
		<h1 class="admin_show_header">All Women</h1>

		<div class="admin_all_men_box">
			<% @women.each do |user| %>
			<%= link_to :controller => "users", :action => "profile", :id => user.id do %>
			<div class="admin_each_men">
				<% if user.avatar.present? %>
					<%= image_tag(user.avatar(:small)) %>
				<% else %>
				<%= image_tag("viewprofile.png") %>
				<% end %>
				<div class="admin_each_men_info">
					<p><%= truncate(user.name, :length => 15) %></p>
					<p><%= user.location %></p>
				</div>
			</div>
			<% end %>
			<% end %>
		</div>

	
	</div>
