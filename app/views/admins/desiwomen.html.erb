<% provide(:title, @admin.name) %>

	<%= link_to profile_admin_path(current_admin) do %>
	  <div class="all_logo">
		<%= image_tag("headerlogo.png") %>	
	  </div>
	<% end %>


	<%= render 'shared/admin_navbar' %>
	<%= render 'shared/allbar' %>

 <div class="admin_cont">

	<%= render 'shared/sidebar' %>


	<% flash.each do |key, value| %>
	  <div class="alert  alert-<%= key %>"><%= value %></div>
	<% end %>	
	
	<div class="admin_show">
		<h1 class="admin_all_men_header">Desi Women</h1>
	  <%= render 'sort_women' %>
		<div class="admin_all_men_box">
			<% @women.each do |user| %>
			<%= link_to :controller => "users", :action => "profile", :id => user.id do %>
			<div class="admin_each_men">
				<% if user.avatar.present? %>
					<%= image_tag(user.avatar(:large)) %>
				<% else %>
				<%= image_tag("viewprofile.png") %>
				<% end %>
				<div class="admin_each_men_info">
					<p><%= truncate(user.name, :length => 15) %></p>
					<p><%= user.location %></p>
				</div>
			</div>
			<% end %>
			<% end %>
		</div>
	</div>
 </div>	
