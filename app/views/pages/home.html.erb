
<div class="container home_wrapper">

	<div class="home_logo">
		<%= image_tag("logonew1.png", :alt => "shiaME Home Page", :class => "home_logo_new") %>

	<div class="invite_code">
		<h1 class="home_tagline"> a matchmaking service </h1>
		<p>To be notified of our Launch, enter your email</p>
		<%= form_for @subscriber do |f| %>
		    <%= f.text_field :email, :value => "Enter Email", :class => "invite_code_field" %>
		    <%= f.submit "SUBSCRIBE", :class => "invite_code_button"  %>
		<% end %>
	</div>

</div>

	<%= link_to about_path do %>
		<div class="what_is_site">
			<p> What is shiaME? </p>
		</div>
	<% end %>

	


</div>
