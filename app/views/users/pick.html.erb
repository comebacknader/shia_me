<% provide(:title, "Sign Up") %>


	<div class="user_all_bar">
	</div>

<div class="container"> 

 <div class="pick_mm_box">
 	<h1 class="pick_mm_header"> Pick Your MatchMaker </h1>
 
 	<% @admins.each do |admin| %>
  	 <div class="user_pick_each_admin">
 	    <p class="pick_mm_name"><strong> <%= admin.name %> </strong> <br />
 	    Ethnicity:  <%= admin.ethnicity %>
 	     </p>
 	    <% if admin.avatar.present? %>
 	    <div class="user_pick_mm_img_div">
 	     <%= image_tag(admin.avatar(:small), class: "user_pick_mm_img") %>
 	    </div>
 	    <% else %>
 	    <div class="user_pick_mm_img_div">
 	      <%= image_tag("viewprofile.png", class: "user_pick_mm_img") %>
		</div>
 	    <% end %>
 	    <p class="pick_mm_bio"> <%= admin.bio %> </p>
	 	     
 	 </div>
			<div class="">	
		<%= form_for @user, :url => { action: "pickmm" } do |f| %>
		
		<%= f.hidden_field :admin_id, :value => admin.id %>	
			
			<%= f.submit "PICK", class: "user_pick_button" %>
				
		<% end %> 	 
 	<% end %>
 </div>

</div>